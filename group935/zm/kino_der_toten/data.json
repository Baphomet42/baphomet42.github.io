        {
            "header": "Song",
            "subsections": [
                {
                    "header": "115",
                    "description": "Interact with 3 Element 115 meteorites.",
                    "elements": [
                        {
                            "pic": {
                                "src": "assets/8.jpg",
                                "alt": "Location 1 - in the Lobby, between the stairs"
                            }
                        },
                        {
